<!-- Loop over each object in the projects array -->
{{#each blogPosts as |blogPost| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2><a href="/blog/{{blogPost.id}}">{{blogPost.name}}</a></h2>
    
  </div>
  <div class="col-md-7">
    <p>
      {{blogPost.description}}
    </p>
  </div>
</div>
{{/each}}
